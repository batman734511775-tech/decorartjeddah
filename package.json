{
  "name": "decorartjeddah",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "purge": "purgecss --css style.css --content \"./*.html\" \"./neighborhoods/**/*.html\" \"./js/**/*.js\" --output dist",
    "minify": "cleancss -o dist/style.min.css dist/style.css",
    "build": "npm run purge && npm run minify",
    "critical": "critical https://decorartjeddah.vercel.app --inline --extract --width 375 --height 667"
  },
  "devDependencies": {
    "clean-css-cli": "^5.6.3",
    "critical": "github:addyosmani/critical#master",
    "purgecss": "^8.0.0"
  }
}
